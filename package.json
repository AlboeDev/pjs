{
  "name": "pjs",
  "version": "0.0.0",
  "description": "Palpable JavaScript Styling",
  "scripts": {
    "build": "esbuild ./src/index.ts --bundle --format=esm --minify --outfile=./build/build.js",
    "build:clean": "rimraf ./build",
    "compile": "tsc",
    "compile:clean": "rimraf ./dist",
    "compile:validate": "tsc --noemit",
    "lint": "eslint ./src/**/*.*",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "start": "echo no start script specified",
    "test": "jest \"src/.*\\.test\\.js\"",
    "test:integration": "jest \"src/.*\\.integration\\.test\\.js\"",
    "test:unit": "jest \"src/.*\\.unit\\.test\\.js\""
  },
  "main": "index.js",
  "repository": "git@github.com:AlboeDev/pjs.git",
  "author": "Alboe Development",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "esbuild": "^0.8.56",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.8",
    "eslint-plugin-tsdoc": "^0.2.11",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
